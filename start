#!/usr/bin/env bash

# Install pip3 Packages
pip3 install -U -r requirements.txt

# BUILD=Variable denoting current build string:
BUILD=`date -u +"%D-%T"`
BUILD="$BUILD-$BUILDER"

echo $BUILD >> BUILD.txt

